/*BASE DE DONNEES UTILISATEURS*/


use bddvigenere;

CREATE TABLE IF NOT EXISTS users(
    id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
    name TEXT,
    email TEXT,
    password TEXT,
    admin BOOL,
    city TEXT,
    privatekey INT
);

 
INSERT INTO users (name, email, password, admin, city, privatekey) VALUES ('pasadmin', 'user@email.com', 'password', 0, 'Paris',1);
INSERT INTO users (name, email, password, admin, city, privatekey) VALUES ('admin', 'admin@email.com', 'password', 1, 'Paris',1);

SELECT * FROM USERS;