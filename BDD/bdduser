/*BASE DE DONNEES UTILISATEURS*/


CREATE TABLE users (
    id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
    name TEXT,
    email TEXT,
    password TEXT,
    admin BOOL,
    city TEXT
);

 
